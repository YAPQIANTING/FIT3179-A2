{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "title": {
        "text": "Unemployment Rate of the Highest 5"
    },
    "width": 300,
    "height": 200,
    "data": {
      "url": "https://raw.githubusercontent.com/YAPQIANTING/FIT3179-A2/main/data.csv"
    },
    "transform": [
      {
        "filter": {
          "field": "Country Name",
          "oneOf": ["Iceland", "Korea, Dem. People's Rep.", "Qatar", "Singapore", "United Arab Emirates"]
        }
      }
    ],
    "mark": {
      "type": "arc",
      "innerRadius": 50
    },
    "encoding": {
      "theta": {"field": "Unemployment Rate", "type": "quantitative"},
      "color": {"field": "Country Name", "type": "nominal", "scale": {"scheme": "blues"}},
      "tooltip": [
        {"field": "Country Name", "title": "Country"},
        {"field": "Unemployment Rate", "title": "Unemployment Rate"},
        {"field": "Labour Force", "title": "Labour Force"},
        {"field": "Population", "title": "Population"}
      ]
    },
    "view": {"stroke": null}
  }
  