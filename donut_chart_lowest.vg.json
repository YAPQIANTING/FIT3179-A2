{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "title": {
        "text": "Unemployment Rate of the Lowest 5"
    },
    "width": 300,
    "height": 200,
    "data": {
      "url": "https://raw.githubusercontent.com/YAPQIANTING/FIT3179-A2/main/data.csv",
      "format": {
        "type": "csv"
      }
    },
    "transform": [
      {
        "filter": {
          "field": "Country Name",
          "oneOf": ["Afghanistan", "Djibouti", "Mauritania", "Somalia", "Yemen, Rep."]
        }
      }
    ],
    "mark": {
      "type": "arc",
      "innerRadius": 50
    },
    "encoding": {
      "theta": {"field": "Unemployment Rate", "type": "quantitative"},
      "color": {"field": "Country Name", "type": "nominal", "scale": {"scheme": "reds"}},
      "tooltip": [
        {"field": "Country Name", "title": "Country"},
        {"field": "Unemployment Rate", "title": "Unemployment Rate (%)"},
        {"field": "Labour Force", "title": "Labour Force"},
        {"field": "Population", "title": "Population"}
      ]
    },
    "view": {"stroke": null}
  }
  